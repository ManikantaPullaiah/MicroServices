package com.common.mapper;

import org.springframework.beans.BeanUtils;
import org.springframework.stereotype.Component;

import com.common.entity.PlanEntity;
import com.common.model.CallDetails;
import com.common.model.Plans;
@Component
public class ModelEntityMapper {
	
	
	public Plans calldetailsEntityToCalldetails(PlanEntity entity)
	{
		Plans calldetails=new Plans();
		BeanUtils.copyProperties(entity, calldetails);
		return calldetails;
	}
	
	
	

}
